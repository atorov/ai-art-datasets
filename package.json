{
  "name": "ai-art-datasets",
  "version": "6.2.0",
  "private": true,
  "description": "AI Art Datasets",
  "repository": {
    "type": "git",
    "url": "git@github.com:atorov/ai-art-datasets.git"
  },
  "author": "atorov",
  "license": "MIT",
  "main": "src/app/index.tsx",
  "scripts": {
    "predev": "npm run check",
    "dev": "nodemon --watch webpack.*.js --exec \"webpack serve --define-process-env-node-env development --env BUILD_ENV=local --env PORT=9080 --progress\"",
    "prebuild": "npm run check",
    "build": "npm run clean && webpack --define-process-env-node-env production --progress && npm run html200",
    "check": "npm run lint",
    "clean": "rm -rf ./build/public",
    "html200": "cp ./build/public/index.html ./build/public/200.html",
    "lint": "eslint --ext .tsx,.ts src/",
    "serve": "npx http-server ./build/public"
  },
  "pre-commit": [
    "check"
  ],
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.49",
    "@mui/material": "^5.0.2",
    "@mui/styles": "^5.0.1",
    "ajv": "^8.11.0",
    "core-js": "^3.8.3",
    "outdated": "^0.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-json-view": "^1.20.4",
    "react-router-dom": "^6.0.1",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.2.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.0.0",
    "mini-css-extract-plugin": "^2.1.0",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.7",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.0.1",
    "pre-commit": "^1.2.2",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.1",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.0.0",
    "whatwg-fetch": "^3.5.0",
    "worker-loader": "^3.0.7"
  }
}
